{
  "extends": "airbnb",
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": 0,
    "new-cap": 0,
    "max-len": 0,
    "no-debugger": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "global-require": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "no-use-before-define": 0,
    "import/newline-after-import": 0,
    "import/no-extraneous-dependencies": 0
  },
  "globals": {
    "db": true,
    "isDev": true,
  }
}
